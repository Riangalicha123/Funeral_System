lize();\n      this.closeDelete();\n    },\n    close() {\n      this.dialog = false;\n      this.$nextTick(() => {\n        this.editedItem = Object.assign({}, this.defaultItem);\n        this.editedIndex = -1;\n      });\n    },\n    closeDelete() {\n      this.dialogDelete = false;\n      this.$nextTick(() => {\n        this.editedItem = Object.assign({}, this.defaultItem);\n        this.editedIndex = -1;\n      });\n    },\n    async save() {\n      if (this.editedIndex > -1) {\n        const response = await axios.post('updatePayment', this.editedItem);\n        console.log(response.data);\n      } else {\n        const response = await axios.post('insertPayment', this.editedItem);\n        console.log(response.data);\n      }\n      this.initialize();\n      this.close();\n    }\n  }\n};","map":{"version":3,"names":["axios","data","search","dialog","dialogDelete","headers","title","align","sortable","key","payments","editedIndex","editedItem","AcceptanceId","LastName","FirstName","Services","Amount","Status","Date","defaultItem","computed","formTitle","watch","val","close","closeDelete","created","initialize","methods","respond","get","error","console","log","editItem","item","indexOf","Object","assign","deleteItem","deleteItemConfirm","response","post","$nextTick","save"],"sources":["C:\\laragon\\www\\GitHub\\Funeral_System\\src\\components\\Admin\\PaymentHistory.vue"],"sourcesContent":["<template>\r\n  <v-container style=\"margin-top:50px;\">\r\n    \r\n<v-data-table\r\n  :headers=\"headers\"\r\n  :items=\"payments\"\r\n  :search=\"search\"\r\n  :sort-by=\"[{ key: 'AcceptanceId', order: 'asc' }]\"\r\n>\r\n\r\n  <template v-slot:top>\r\n    <v-toolbar\r\n      flat\r\n    >\r\n    \r\n    <h3>Payment History</h3>\r\n      <v-divider\r\n        class=\"mx-4\"\r\n        inset\r\n        vertical\r\n      ></v-divider>\r\n      <v-spacer></v-spacer>\r\n      \r\n      \r\n      <v-dialog\r\n        v-model=\"dialog\"\r\n        max-width=\"500px\"\r\n      >\r\